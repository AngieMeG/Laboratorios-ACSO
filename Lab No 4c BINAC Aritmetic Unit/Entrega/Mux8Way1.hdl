/**
 * 8-way 15-bit multiplexor.  
 * out = a if sel == 000
 *       b if sel == 001
 *       etc.
 *       h if sel == 111
 */

 
CHIP Mux8Way1 {

    IN  a, b, c, d,
        e, f, g, h,
        sel[3];

    OUT out;

    BUILTIN Mux8Way16;
}