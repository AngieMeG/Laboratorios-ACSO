	// File name: tools/builtIn/And16.hdl
	
	/**
	 * 16-bit-wise And gate: for i  =  0..15: out[i]  =  a[i] and b[i]
	 */
	
CHIP And15 {
	
	    IN  a[15],  b[15];
	    OUT out[15];
	
	    BUILTIN And;
}
	
